{
 "compilerOptions": {
  "target": "esNext",
  "module": "esNext",
  "declaration": true,
  "outDir": "../dist",
  "moduleResolution": "node",
  "baseUrl": "./",
  "types": [
   "@babylonjs/core",
   "threadcomm",
   "divine-binary-tags",
   "divine-binary-object",
   "divine-shaders",
   "divine-hooks"
  ],
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strictPropertyInitialization": false,
  "strict": true,
  "skipLibCheck": true
 }
}
